	"editor.tokenColorCustomizations": {
		"strings": "#191313",
		"functions": "#bd5d00",
		"keywords": "#bd5d00",
		"types": "#749998",
		"variables": "#191313",
		"comments": "#433a3a",
		"numbers": "#934682",
		"textMateRules": [
			{
				"name": "modifier storage",
				"scope": [
					"storage.modifier"
				],
				"settings": {
					"foreground": "#a74e61"
				}
			},
			{
				"scope": [
					"variable.language.this"
				],
				"settings": {
					"fontStyle": "italic",
					"foreground": "#191313"
				}
			},
			{
				"scope": [
					"keyword.operator.arithmetic"
				],
				"settings": {
					"foreground": "#c00828"
				}
			},
			{
				"scope": [
					"constant.language.null"
				],
				"settings": {
					"fontStyle": "italic",
					"foreground": "#be7bbd"
				}
			},
			{
				"name": "block definition",
				"scope": [
					"punctuation.definition.block"
				],
				"settings": {
					"foreground": "#eac096"
				}
			},
			{
				"scope": [
					"meta.method.declaration"
				],
				"settings": {
					"foreground": "#2asd37"
				}
			},
			{
				"scope": [
					"punctuation.definition.typeparameters"
				],
				"settings": {
					"foreground": "#749998"
				}
			}
		]
	},
	"workbench.colorTheme": "Visual Studio Light",
	"vim.easymotion": true,
	"vim.incsearch": true,
	"vim.useSystemClipboard": true,
	"vim.useCtrlKeys": true,
	"vim.hlsearch": true,
	"vim.leader": "<space>",
	"vim.handleKeys": {
		"<space>": true
	},
	"extensions.experimental.affinity": {
		"vscodevim.vim": 1
	}
}
